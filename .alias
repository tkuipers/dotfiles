alias findeti='/usr/bin/find . | grep "eti" | grep '
alias findetx='/usr/bin/find . | grep "etx" | grep '
alias findfile='/usr/bin/find . | grep '
alias add='git add -A'
alias co='git commit'
alias push='git push'
alias st='git status'
alias ci='vi'
alias ins='sudo apt-get install'
alias bd='task burndown.daily'
alias cal='task calendar'
alias t='task'
alias tr='task ready'
alias ta='task all'
alias pipes='pipes.sh -t3 -p3 -r40000 -s7 -f100'
alias weather='curl wttr.in/Edmonton'
alias moon='curl wttr.in/Moon'
alias config='vi ~/.config/i3/config'
alias postman='Postman'
