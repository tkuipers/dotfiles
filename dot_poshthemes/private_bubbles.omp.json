{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#29315a",
          "foreground": "#f5c2e7",
          "leading_diamond": "\uE0B6",
          "style": "diamond",
          "template": "{{ .UserName }}",
          "trailing_diamond": "\uE0B4",
          "type": "session"
        },
        {
          "background": "#29315A",
          "foreground": "#89dceb",
          "leading_diamond": " \uE0B6",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": "\ue5ff {{ .Path }}",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "background": "#29315A",
          "foreground": "#a6e3a1",
          "leading_diamond": " \ue0b6",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": "\ue725 {{ .HEAD }}",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
        "alignment": "right",
        "segments": [
            {
                "background": "#29315A",
                "foreground": "#f5e0dc",
                "leading_diamond": "\ue0b6",
                "trailing_diamond": "\ue0b4",
                "properties": {
                    "linux": "\ue712",
                    "macos": "\ue711",
                    "windows": "\ue70f"
                },
                "style": "diamond",
                "template": "",
                "type": "os"
            },
            {
                "background": "#29315A",
                "foreground": "#89b4fa",
                "leading_diamond": "\ue0b2",
                "properties": {
                    "time_format": "_2,15:04"
                },
                "style": "diamond",
                "template": " \uf5ef {{ .CurrentDate | date .Format }} ",
                "trailing_diamond": "\ue0b4",
                "type": "time"
            }
        ],
        "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#29315A",
          "foreground": "#89dceb",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "austin",
            "threshold": 150
          },
          "style": "diamond",
          "template": "{{ .FormattedMs }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "executiontime"
        },
        {
          "background": "#29315A",
          "foreground": "#fab387",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\uf292",
          "trailing_diamond": "\ue0b4",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
